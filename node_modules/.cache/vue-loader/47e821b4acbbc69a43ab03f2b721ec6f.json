\n\r\n    <h1 class=\"Quiz\">HSC Exam</h1>\r\n    <h2 class=\"Name\">Anonymous</h2>\r\n\r\n  <div class=\"table\">\r\n    <b-table :items=\"items\"></b-table> \r\n  </div>\r\n\r\n\r\n  <div class=\"result\">\r\n    <p>\r\n      Correct:\r\n      <strong>{{ totalCorrect }}</strong>\r\n    </p>\r\n  </div>\r\n\r\n  <div class=\"result\">\r\n    <p>\r\n      Marks Deducted:\r\n      <strong>{{ totalWrong }}</strong>\r\n    </p>\r\n  </div>    \r\n\r\n  <div class=\"result\">\r\n    <p>\r\n      Total:\r\n      <strong>{{ totalMarks }}/40</strong>\r\n    </p>\r\n  </div>    \r\n\r\n</div>\r\n\r\n\r\n</template>\r\n\r\n<script>\r\nimport marks from \"./json/data.json\";\r\n\r\nexport default {\r\n  name: \"student_marks\",\r\n  data()\r\n  {\r\n    return {\r\n      items: marks\r\n      \r\n    };\r\n  },\r\n\r\ncomputed: {\r\n   totalMarks(){\r\n     var correct = 0\r\n     var wrong = 0\r\n     var total = 0\r\n     if(this.items){\r\n       this.items.forEach(element => {\r\n       correct += parseInt(element.Correct)\r\n       wrong += parseInt(element.Wrong)\r\n     })\r\n     total = correct - (0.25*wrong)\r\n     }\r\n     return total\r\n   },\r\n\r\n   totalCorrect(){\r\n     var tc = 0\r\n     if(this.items){\r\n       this.items.forEach(element => {\r\n       tc += parseInt(element.Correct)\r\n     })\r\n     }\r\n     return tc\r\n   },\r\n\r\n   totalWrong(){\r\n     var tw = 0\r\n     if(this.items){\r\n       this.items.forEach(element => {\r\n       tw += parseInt(element.Wrong)\r\n     })\r\n     }\r\n     return (tw*0.25)\r\n   }\r\n\r\n   \r\n}\r\n\r\n\r\n\r\n\r\n};\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped>\r\n  .result{\r\n    margin-left: 35%;\r\n    font-size: 2rem;\r\n  }\r\n  .table{\r\n    width: 70%;\r\n    margin-left: 15%;\r\n  }  \r\n</style>\r\n"]}]}